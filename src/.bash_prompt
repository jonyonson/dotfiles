# Git Prompt
# ========================================

# Using \[ and \] around colors is necessary to prevent issues with command lin
# issues with command line editing/browsing/completion!
Red='\[\e[31m\]'
Green='\[\e[32m\]'
Yellow='\[\e[1;33m\]'
Blue='\[\e[1;34m\]'
Magenta='\[\e[1;35m\]'
Cyan='\[\e[1;36m\]'
White='\[\e[1;37m\]'
Clear='\[\e[0m\]'

[ -r ~/.git-prompt.sh ] && source ~/.git-prompt.sh

PROMPT_COMMAND='__git_ps1 "${Green}âžœ  ${Yellow}\w${Clear}" " "'



